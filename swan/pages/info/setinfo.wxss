
.banner {
  width: 100%;
  height: 200rpx;
  background: #ff552e;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  color: #fff;
  font-size: 48rpx;
}
.item-box {
  padding: 20rpx;
}

.title {
  font-size: 48rpx;
  color: #333;
  margin-bottom: 40rpx;
}

.requised {
  color: red;
}

.item-label-box {
  font-size: 32rpx;
}

.input-box {
  border-bottom: 1px solid #ccc;
  margin-bottom: 30rpx;
}

.weui-input {
  height: 100rpx;
  padding: 0 10rpx;
  font-size: 36rpx;
}

.placeholderClass {
  font-size: 36rpx;
  color: #ccc;
}

.labelwenti {
  margin: 20rpx 10rpx;
  padding: 0 0rpx;
  font-size: 32rpx;
}

.radio-box {
  margin-left: 30rpx;
  font-size: 32rpx;
}

.valuelist {
  margin: 10rpx 0;
}

.address-box {
  border: 1px dashed #bbb;
  /* margin: 0 40rpx; */
  height: 200rpx;
}

.temperature {
  border: 1px dashed #bbb;
  height: 100rpx;
  line-height: 100rpx;
}

.submitbtn {
  background: #ff552e;
  color: #fff;
  margin-bottom: 20rpx;
}
.companyname{
  padding:  0 20rpx;
  color: #256589;
}
